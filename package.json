{
  "name": "vue-tinykeys",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.2.0",
  "description": "Vue composable and directive for keybindings.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-tinykeys#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-tinykeys.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-tinykeys/issues",
  "keywords": [
    "hotkey",
    "bindings",
    "keys",
    "shortcut"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "tinykeys": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@types/node": "^18.16.7",
    "@vitejs/plugin-vue": "^4.2.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.5",
    "vue": "^3.2.47"
  }
}
